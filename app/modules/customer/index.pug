doctype html
html(lang='en')
  head
    // Required meta tags
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
    // Title Page
    title Dashboard
    // Fontfaces CSS
    link(href='/assets/customer/css/font-face.css', rel='stylesheet', media='all')
    link(href='/assets/customer/vendor/font-awesome-4.7/css/font-awesome.min.css', rel='stylesheet', media='all')
    link(href='/assets/customer/vendor/font-awesome-5/css/fontawesome-all.min.css', rel='stylesheet', media='all')
    link(href='/assets/customer/vendor/mdi-font/css/material-design-iconic-font.min.css', rel='stylesheet', media='all')
    // Bootstrap CSS
    link(href='/assets/customer/vendor/bootstrap-4.1/bootstrap.min.css', rel='stylesheet', media='all')
    // Vendor CSS
    link(href='/assets/customer/vendor/animsition/animsition.min.css', rel='stylesheet', media='all')
    link(href='/assets/customer/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css', rel='stylesheet', media='all')
    link(href='/assets/customer/vendor/wow/animate.css', rel='stylesheet', media='all')
    link(href='/assets/customer/vendor/css-hamburgers/hamburgers.min.css', rel='stylesheet', media='all')
    link(href='/assets/customer/vendor/slick/slick.css', rel='stylesheet', media='all')
    link(href='/assets/customer/vendor/select2/select2.min.css', rel='stylesheet', media='all')
    link(href='/assets/customer/vendor/perfect-scrollbar/perfect-scrollbar.css', rel='stylesheet', media='all')
    // Main CSS
    link(href='/assets/customer/css/theme.css', rel='stylesheet', media='all')
  body.animsition
    .page-wrapper
      // HEADER MOBILE
      header.header-mobile.d-block.d-lg-none
        .header-mobile__bar
          .container-fluid
            .header-mobile-inner
              a.logo(href='index.html')
                img(src='/assets/customer/images/icon/logo.png', alt='CoolAdmin')
              button.hamburger.hamburger--slider(type='button')
                span.hamburger-box
                  span.hamburger-inner
        nav.navbar-mobile
          .container-fluid
            ul.navbar-mobile__list.list-unstyled
              li
                a.js-arrow(href='#')
                  i.fas.fa-tachometer-alt
                  | Dashboard
              li
                a.js-arrow(href='#')
                  i.fas.fa-cog
                  | Change password
      // END HEADER MOBILE
      // MENU SIDEBAR
      aside.menu-sidebar.d-none.d-lg-block
        .logo
          a(href='#')
            img(src='/assets/customer/images/icon/logo.png', alt='Cool Admin')
        .menu-sidebar__content.js-scrollbar1
          nav.navbar-sidebar
            ul.list-unstyled.navbar__list
              li.active
                a.js-arrow(href='#')
                  i.fas.fa-tachometer-alt
                  | Dashboard
              li
                a.js-arrow(href='#', data-toggle='modal', data-target='#changePasswordModal')
                  i.fas.fa-cog
                  | Change password
      // END MENU SIDEBAR
      // PAGE CONTAINER
      .page-container
        // HEADER DESKTOP
        header.header-desktop
          .section__content.section__content--p30
            .container-fluid
              .header-wrap
                .offset-md-4.offset-lg-4
                .header-button
                  .noti-wrap
                    .noti__item.js-item-menu
                      i.zmdi.zmdi-comment-more
                      span.quantity 1
                      .mess-dropdown.js-dropdown
                        .mess__title
                          p You have 2 news message
                        .mess__item
                          .image.img-cir.img-40
                            img(src='/assets/customer/images/icon/avatar-06.jpg', alt='Michelle Moreno')
                          .content
                            h6 Michelle Moreno
                            p Have sent a photo
                            span.time 3 min ago
                        .mess__item
                          .image.img-cir.img-40
                            img(src='/assets/customer/images/icon/avatar-04.jpg', alt='Diane Myers')
                          .content
                            h6 Diane Myers
                            p You are now connected on message
                            span.time Yesterday
                        .mess__footer
                          a(href='#') View all messages
                    .noti__item.js-item-menu
                      i.zmdi.zmdi-email
                      span.quantity 1
                      .email-dropdown.js-dropdown
                        .email__title
                          p You have 3 New Emails
                        .email__item
                          .image.img-cir.img-40
                            img(src='/assets/customer/images/icon/avatar-06.jpg', alt='Cynthia Harvey')
                          .content
                            p Meeting about new dashboard...
                            span Cynthia Harvey, 3 min ago
                        .email__item
                          .image.img-cir.img-40
                            img(src='/assets/customer/images/icon/avatar-05.jpg', alt='Cynthia Harvey')
                          .content
                            p Meeting about new dashboard...
                            span Cynthia Harvey, Yesterday
                        .email__item
                          .image.img-cir.img-40
                            img(src='/assets/customer/images/icon/avatar-04.jpg', alt='Cynthia Harvey')
                          .content
                            p Meeting about new dashboard...
                            span Cynthia Harvey, April 12,,2018
                        .email__footer
                          a(href='#') See all emails
                    .noti__item.js-item-menu
                      i.zmdi.zmdi-notifications
                      span.quantity 3
                      .notifi-dropdown.js-dropdown
                        .notifi__title
                          p You have 3 Notifications
                        .notifi__item
                          .bg-c1.img-cir.img-40
                            i.zmdi.zmdi-email-open
                          .content
                            p You got a email notification
                            span.date April 12, 2018 06:50
                        .notifi__item
                          .bg-c2.img-cir.img-40
                            i.zmdi.zmdi-account-box
                          .content
                            p Your account has been blocked
                            span.date April 12, 2018 06:50
                        .notifi__item
                          .bg-c3.img-cir.img-40
                            i.zmdi.zmdi-file-text
                          .content
                            p You got a new file
                            span.date April 12, 2018 06:50
                        .notifi__footer
                          a(href='#') All notifications
                  .account-wrap
                    .account-item.clearfix.js-item-menu
                      .image
                        img(src='/assets/customer/images/icon/avatar-01.jpg', alt='John Doe')
                      .content
                        a.js-acc-btn(href='#') Customer
                      .account-dropdown.js-dropdown
                        .info.clearfix
                          .image
                            a(href='#')
                              img(src='/assets/customer/images/icon/avatar-01.jpg', alt='John Doe')
                          .content
                            h5.name
                              a(href='#') Customer
                            span.email customer@heremail.com
                        .account-dropdown__body
                          .account-dropdown__item
                            a(href='#')
                              i.zmdi.zmdi-account
                              | Account
                          .account-dropdown__item
                            a(href='#')
                              i.zmdi.zmdi-settings
                              | Setting
                          .account-dropdown__item
                            a(href='#')
                              i.zmdi.zmdi-money-box
                              | Billing
                        .account-dropdown__footer
                          a(href='#')
                            i.zmdi.zmdi-power
                            | Logout
        // HEADER DESKTOP
        // MAIN CONTENT
        .main-content
          .section__content.section__content--p30
            .container-fluid
              .row
                .col-md-12
                  .overview-wrap
                    h2.title-1 Dashboard
              .row.m-t-25
                .col-sm-6.col-lg-8
                  button(type='button', data-toggle='modal', data-target='#servicesModal')
                    .overview-item.overview-item--c2
                      .overview__inner
                        .overview-box.clearfix
                          .icon
                            i.zmdi.zmdi-washing-machine
                          .text
                            h2 SERVICES
                            small
                              span OFFERED
        // end MAIN CONTENT
        // SERVICES MODAL
        #servicesModal.modal.fade(tabindex='-1', role='dialog', aria-hidden='true')
          .modal-dialog.modal-lg(role='document')
            .modal-content
              .modal-header
                h5.modal-title SERVICES OFFERED
                button.close(type='button', data-dismiss='modal', aria-label='Close')
                  span(aria-hidden='true') Ã—
              .modal-body
                .container-fluid
                  .row
                    // WASH AND DRY CARD
                    .col-md-6
                      .card
                        img.card-img-top(src='/assets/customer/images/wash-and-dry.jpg', alt='Card image cap')
                        .card-body
                          h4.card-title.mb-3
                            label.form-check-label.offset-md-1.offset-xs-1(for='checkbox1')
                              input#washAndDry.form-check-input(type='checkbox', name='washAndDry', value='washAndDry')
                              | Wash and Dry
                          p.card-text
                            | Lorem ipsum, dolor sit amet consectetur adipisicing elit. Nobis provident asperiores totam? Consectetur impedit illo nemo veritatis unde modi quis!
                    // end WASH AND DRY CARD
                    // FOLD CARD
                    .col-md-6
                      .card
                        img.card-img-top(src='/assets/customer/images/folding.jpg', alt='Card image cap')
                        .card-body
                          h4.card-title.mb-3
                            label.form-check-label.offset-md-1.offset-xs-1(for='checkbox1')
                              input#checkbox1.form-check-input(type='checkbox', name='checkbox1', value='option1')
                              | Folding
                          p.card-text
                            | Lorem ipsum, dolor sit amet consectetur adipisicing elit. Nobis provident asperiores totam? Consectetur impedit illo nemo veritatis unde modi quis!
                    // end FOLD CARD
                    // IRONING CARD
                    .col-md-6
                      .card
                        img.card-img-top.img-fluid(src='/assets/customer/images/ironing.jpg', alt='Card image cap')
                        .card-body
                          h4.card-title.mb-3
                            label.form-check-label.offset-md-1.offset-xs-1(for='checkbox1')
                              input#ironing.form-check-input(type='checkbox', name='ironing', value='ironing')
                              | Ironing
                          p.card-text
                            | Lorem ipsum, dolor sit amet consectetur adipisicing elit. Nobis provident asperiores totam? Consectetur impedit illo nemo veritatis unde modi quis!
                    // end IRONING CARD
                    // DELIVERY CARD
                    .col-md-6
                      .card
                        img.card-img-top.img-fluid(src='/assets/customer/images/delivery.jpg', alt='Card image cap')
                        .card-body
                          h4.card-title.mb-3
                            label.form-check-label.offset-md-1.offset-xs-1(for='checkbox1')
                              input#delivery.form-check-input(type='checkbox', name='delivery', value='delivery')
                              | Delivery
                          p.card-text
                            | Lorem ipsum, dolor sit amet consectetur adipisicing elit. Nobis provident asperiores totam? Consectetur impedit illo nemo veritatis unde modi quis!
                    // end DELIVERY CARD
              .modal-footer
                button.btn.btn-secondary(type='button', data-dismiss='modal')
                  i.fa.fa-times
                  |  Cancel
                button.btn.btn-success(type='button', data-toggle='modal', data-target='#servicesConfirmModal', onclick='closeServiceModal()')
                  i.fa.fa-check
                  |  Confirm
        // end SERVICES MODAL
        // CONFIRM SERVICES MODAL
        #servicesConfirmModal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='mediumModalLabel', aria-hidden='true')
          .modal-dialog.modal-lg(role='document')
            .modal-content
              .modal-header
                h5#mediumModalLabel.modal-title Computation
                button.close(type='button', data-dismiss='modal', aria-label='Close')
                  span(aria-hidden='true') Ã—
              .modal-body
                .container-fluid
                  .row
                    .col-md-4
                      input.form-control(type='text', placeholder='How many kgs?')
                    .col-md-4
                      input.form-control(type='text', placeholder='Price', disabled='')
                    .col-md-4
                      input.form-control(type='text', placeholder='Total Price', disabled='')
                  .row
                    .col-md-4
                      input.form-control(type='text', placeholder='Blankets')
                    .col-md-4
                      input.form-control(type='text', placeholder='Pieces', disabled='')
                    .col-md-4
                      input.form-control(type='text', placeholder='Total Price', disabled='')
              .modal-footer
                button.btn.btn-secondary(type='button', onclick='showServiceModal()') Cancel
                button.btn.btn-primary(type='button') Confirm
        // end CONFIRM SERVICES MODAL
        // CHANGE PASSWORD MODAL
        #changePasswordModal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='mediumModalLabel', aria-hidden='true')
          .modal-dialog.modal-lg(role='document')
            .modal-content
              .modal-header
                h5#mediumModalLabel.modal-title Change Password
                button.close(type='button', data-dismiss='modal', aria-label='Close')
                  span(aria-hidden='true') Ã—
              .modal-body
                .form-group
                  .input-group
                    .input-group-addon
                      i.fa.fa-asterisk
                    input#oldPassword.form-control(type='password', name='oldPassword', placeholder='Old Password')
                .form-group
                  .input-group
                    .input-group-addon
                      i.fa.fa-asterisk
                    input#newPassword.form-control(type='password', name='newPassword', placeholder='New Password')
                .form-group
                  .input-group
                    .input-group-addon
                      i.fa.fa-asterisk
                    input#confirmPassword.form-control(type='password', name='confirmPassword', placeholder='Confirm Password')
              .modal-footer
                button.btn.btn-secondary(type='button', data-dismiss='modal')
                  i.fa.fa-times
                  |  Cancel
                button.btn.btn-primary(type='button')
                  i.fa.fa-check
                  |  Confirm
        // end CHANGE PASSWORD MODAL
        // Jquery JS
        script(src='/assets/customer/vendor/jquery-3.2.1.min.js')
        // Bootstrap JS
        script(src='/assets/customer/vendor/bootstrap-4.1/popper.min.js')
        script(src='/assets/customer/vendor/bootstrap-4.1/bootstrap.min.js')
        // Vendor JS
        script(src='/assets/customer/vendor/slick/slick.min.js')
        script(src='/assets/customer/vendor/wow/wow.min.js')
        script(src='/assets/customer/vendor/animsition/animsition.min.js')
        script(src='/assets/customer/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js')
        script(src='/assets/customer/vendor/counter-up/jquery.waypoints.min.js')
        script(src='/assets/customer/vendor/counter-up/jquery.counterup.min.js')
        script(src='/assets/customer/vendor/circle-progress/circle-progress.min.js')
        script(src='/assets/customer/vendor/perfect-scrollbar/perfect-scrollbar.js')
        script(src='/assets/customer/vendor/chartjs/Chart.bundle.min.js')
        script(src='/assets/customer/vendor/select2/select2.min.js')
        // Main JS
        script(src='/assets/customer/js/main.js')
        // end document
        script.
          function closeServiceModal() {
          $("#servicesModal").modal("hide");
          console.log('closeService');
          }
          function showServiceModal() {
          $("#servicesConfirmModal").modal("hide");
          $("#servicesModal").modal("show");
          console.log('showService');
          }
