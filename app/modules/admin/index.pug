        // CONFIRM SERVICES MODAL
        #servicesConfirmModal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='mediumModalLabel', aria-hidden='true')
          .modal-dialog.modal-lg(role='document')
            .modal-content
              .modal-header
                h5#mediumModalLabel.modal-title Computation
                button.close(type='button', data-dismiss='modal', aria-label='Close')
                  span(aria-hidden='true') ×
              .modal-body
                .container-fluid
                  .row
                    .col-md-4
                      input.form-control(type='text', placeholder='How many kgs?')
                    .col-md-4
                      input.form-control(type='text', placeholder='Price', disabled='')
                    .col-md-4
                      input.form-control(type='text', placeholder='Total Price', disabled='')
                  .row
                    .col-md-4
                      input.form-control(type='text', placeholder='Blankets')
                    .col-md-4
                      input.form-control(type='text', placeholder='Pieces', disabled='')
                    .col-md-4
                      input.form-control(type='text', placeholder='Total Price', disabled='')
              .modal-footer
                button.btn.btn-secondary(type='button', onclick='showServiceModal()') Cancel
                button.btn.btn-primary(type='button') Confirm
        // end CONFIRM SERVICES MODAL
        // CHANGE PASSWORD MODAL
        #changePasswordModal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='mediumModalLabel', aria-hidden='true')
          .modal-dialog.modal-lg(role='document')
            .modal-content
              .modal-header
                h5#mediumModalLabel.modal-title Change Password
                button.close(type='button', data-dismiss='modal', aria-label='Close')
                  span(aria-hidden='true') ×
              .modal-body
                .form-group
                  .input-group
                    .input-group-addon
                      i.fa.fa-asterisk
                    input#oldPassword.form-control(type='password', name='oldPassword', placeholder='Old Password')
                .form-group
                  .input-group
                    .input-group-addon
                      i.fa.fa-asterisk
                    input#newPassword.form-control(type='password', name='newPassword', placeholder='New Password')
                .form-group
                  .input-group
                    .input-group-addon
                      i.fa.fa-asterisk
                    input#confirmPassword.form-control(type='password', name='confirmPassword', placeholder='Confirm Password')
              .modal-footer
                button.btn.btn-secondary(type='button', data-dismiss='modal')
                  i.fa.fa-times
                  |  Cancel
                button.btn.btn-primary(type='button')
                  i.fa.fa-check
                  |  Confirm
